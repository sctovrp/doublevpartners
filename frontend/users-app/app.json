{
  "expo": {
    "name": "users-app",
    "slug": "users-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sctovarp.usersapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "8fb8f3a4-6c56-480e-8969-d5b83494f610"
      }
    }
  },
  "plugins": [
    "expo-font",
    {
      "fonts": [
        "node_modules/@expo-google-fonts/poppins/Poppins_400Regular.ttf"
      ]
    }
  ]
}
